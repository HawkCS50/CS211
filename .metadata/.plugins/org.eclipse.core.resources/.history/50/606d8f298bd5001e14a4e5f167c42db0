package labDigitMath;

public class methods {
	public int thouPlace1(int x) {
		return (x/1000)%10;
	}
	public int thouPlace2(int x) {
		return (x%10000)/1000;
	}
	public int digitCount(int x) {
		int i=0;
		for (;Math.abs(x)>1;i++)
			x/=10;
		return i;
	}
	public int digitCountRecurs(int x) {
		if (Math.abs(x)>1)
			return 1+digitCountRecurs(x/10);
		else
			return 0;
	}
}
